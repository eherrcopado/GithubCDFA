                <th scope="col" style="border-bottom: 1px solid #333;">Expiration Date</th>
              </tr>
            </thead>
            <tbody>
                <apex:repeat value="{!accountContactRecords}" var="accountContact">
              <tr style="line-height: 26px; border-top: 1px solid #000;">                
                <td><apex:outputText value="{!accountContact.Contact.FirstName}"/></td>
                <td><apex:outputText value="{!accountContact.Contact.MiddleName}" /></td>
                <td><apex:outputText value="{!accountContact.Contact.LastName}" /></td>
                <td><apex:outputText value="{!accountContact.Contact.License_No__c}"/></td>
                
                <td><apex:outputText value="{0,date,MM/dd/yy}"> <apex:param value="{!accountContact.Contact.License_To_Date__c}" /></apex:outputText></td>
