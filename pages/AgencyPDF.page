    <span><apex:outputText value="{!associatePrimaryType[0].Location.VisitorAddress.State}"/></span> 
    <span><apex:outputText value="{!associatePrimaryType[0].Location.VisitorAddress.PostalCode}"/></span>
    <div><apex:outputText value="{!accRec.phone}" /></div>
</div>
