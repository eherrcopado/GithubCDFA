        </connector>
        <fields>
            <name>message_0</name>
            <fieldText>&lt;p&gt;To ensure the security of your account, we need to verify your identity.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;We have sent a verification code to the email account you registered on this system.&lt;/p&gt;&lt;p&gt;Please retrieve the email and enter the code in the space below.&lt;/p&gt;&lt;p&gt;If you require assistance, contact&amp;nbsp;&lt;a href=&quot;mailto:RSA@cdfa.ca.gov&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot; style=&quot;font-family: inherit; color: rgb(5, 99, 193);&quot;&gt;RSA@cdfa.ca.gov&lt;/a&gt;.&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>WarningMessage_new</name>
            <fieldText>&lt;p&gt;&lt;strong style=&quot;background-color: rgb(255, 255, 255); color: rgb(255, 8, 8); font-size: 14px;&quot;&gt;{!invalidAttempt} of 3 invalid login attempts recorded; if you exceed 3 invalid attempts, your account will be locked.&lt;/strong&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>invalidAttempt</leftValueReference>
                    <operator>GreaterThan</operator>
                    <rightValue>
                        <numberValue>0.0</numberValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>invalidAttempt</leftValueReference>
                    <operator>LessThan</operator>
                    <rightValue>
                        <numberValue>3.0</numberValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
